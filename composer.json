{
  "name": "zetta-code/tss-authentication",
  "type": "library",
  "description": "TSS/Authentication",
  "license": "MIT",
  "keywords": [
    "zf2",
    "tss",
    "authentication"
  ],
  "homepage": "https://github.com/zetta-code/tss-authentication",
  "authors": [
    {
      "name": "Thiago S. Santos",
      "email": "thiagos.xsantos@gmail.com",
      "homepage": "http://thiagossantos.com.br",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "TSS\\Authentication\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-stdlib": "^3.0",
    "zendframework/zend-session": "^2.7",
    "zendframework/zend-permissions-acl": "^2.6",
    "zendframework/zend-mvc-plugin-flashmessenger": "^1.0",
    "doctrine/doctrine-orm-module": "dev-master",
    "zetta-code/tss-bootstrap": "dev-develop",
    "zetta-code/tss-doctrineutil": "dev-master"
  },
  "require-dev": {
    "zendframework/zend-mvc-i18n": "^1.0"
  },
  "extra": {
    "zf": {
      "module": "TSS\\Authentication",
      "config-provider": "TSS\\Authentication\\ConfigProvider"
    }
  }
}
